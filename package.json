{
  "name": "cantina",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec babel-node server.js",
    "build": "babel src --out-dir dist/src && babel routes --out-dir dist/routes && babel server.js --out-dir dist",
    "clean": "rm -rf dist/",
    "copy": "cp package.json dist/ && cp -R views/ dist/views/ && cp -R public/ dist/public/",
    "build:prod": "npm run clean && npm run build && npm run copy"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv-safe": "^6.1.0",
    "express": "~4.16.0",
    "express-handlebars": "^3.1.0",
    "handlebars-intl": "^1.1.2",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.5.9",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.19.0"
  }
}